{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "useDefineForClassFields": true,
    "strict": true,
    "resolveJsonModule": true,
    "moduleResolution": "Node",
    "isolatedModules": true,
    "esModuleInterop": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "noEmit": true,
  }
}